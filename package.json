{
  "name": "natours",
  "version": "1.0.0",
  "description": "Backend - express js",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "preserve": "npm run build",
    "serve": "concurrently \"npx tsc -w\"  \"gulp && nodemon dist/server.js\"",
    "import-to-db": "node ./dev-data/data/import-dev-data.js --import",
    "delete-from-db": "node ./dev-data/data/import-dev-data.js --delete"
  },
